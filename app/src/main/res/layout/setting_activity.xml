<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
       android:text="设置中心"
        style="@style/TitleStye"/>

   <android.ye.mobileguard.View.SetItemView
    xmlns:mobileguard="http://schemas.android.com/apk/res/android.ye.mobileguard"
    android:id="@+id/set_update"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    mobileguard:destitle="设置自动更新"
    mobileguard:desoff="自动更新已关闭"
    mobileguard:deson="自动更新已开启"
    >
    </android.ye.mobileguard.View.SetItemView>

    <android.ye.mobileguard.View.SetItemView
    xmlns:mobileguard="http://schemas.android.com/apk/res/android.ye.mobileguard"
    android:id="@+id/dis_addr"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    mobileguard:destitle="电话归属地显示设置"
    mobileguard:desoff="归属地显示已关闭"
    mobileguard:deson="归属地显示已开启"
    >
    </android.ye.mobileguard.View.SetItemView>

    <android.ye.mobileguard.View.ClickItemView
    android:id="@+id/dis_style"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    </android.ye.mobileguard.View.ClickItemView>

 <android.ye.mobileguard.View.ClickItemView
    android:id="@+id/dis_location"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    </android.ye.mobileguard.View.ClickItemView>

    <android.ye.mobileguard.View.SetItemView
        xmlns:mobileguard="http://schemas.android.com/apk/res/android.ye.mobileguard"
        android:id="@+id/set_black_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobileguard:destitle="黑名单拦截设置"
        mobileguard:desoff="拦截短信与黑名单电话已关闭"
        mobileguard:deson="拦截短信与黑名单电话已开启"
        />
    <android.ye.mobileguard.View.SetItemView
        xmlns:mobileguard="http://schemas.android.com/apk/res/android.ye.mobileguard"
        android:id="@+id/set_app_lock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobileguard:destitle="程序锁设置"
        mobileguard:desoff="程序锁已关闭"
        mobileguard:deson="程序锁已开启"
        />
    <android.ye.mobileguard.View.SetItemView
        xmlns:mobileguard="http://schemas.android.com/apk/res/android.ye.mobileguard"
        android:id="@+id/set_block_sms"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobileguard:destitle="本应用替代短信应用"
        mobileguard:desoff="可以接收短信"
        mobileguard:deson="屏蔽所有短信"
        />



</LinearLayout>